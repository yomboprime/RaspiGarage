{
    "version": 1,
    "languageCode": "es",
    "translationEncodingAlias": "utf8",
    "intruderAlert": true,
    "modules": {
        "Camera": {
            "cameraDevice": "/dev/video0",
            "cameraWidth": 640,
            "cameraHeight": 480,
            "cameraFPS": 5,
            "captureTempPath": "./tempCaptures/"
        },
        "RelayTimer": {
			"relayPin": 1,
			"setPinAsOutputCommandCWD1": "./scripts/",
			"setPinAsOutputCommand1": "python turnOnOffGPIO.py <pinValue>",
			"setPinStateCommandCWD1": "./scripts/",
			"setPinStateCommand1": "python turnOnOffGPIO.py <pinValue>",

			"setPinAsOutputCommandCWD": "./",
			"setPinAsOutputCommand": "gpio mode <pin> out",
			"setPinStateCommandCWD": "./",
			"setPinStateCommand": "gpio write <pin> <pinValue>"
        }
    }
}